<MTabs Height="90" Value="Value"
       ValueChanged="ValueChanged"
       ActiveClass="text-h6 font-weight-bold"
       HideSlider>
    @foreach (var item in Items)
    {
        <MTab @key="item"
              Class="app-tab"
              Ripple="false">
            @item.Title
        </MTab>
    }
</MTabs>

